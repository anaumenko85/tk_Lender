	private WebDriver driver;
	
	@FindBy(how=How.XPATH, using="/html/body/section[1]/elmenu[1]/div[2]/div[2]/ng-view[1]/div[1]/split-toolbar[1]/nav[1]/form[1]/a[1]")
	public WebElement btnAddUser;
	
	public SystemPage(WebDriver driver) {
		// TODO Auto-generated constructor stub
		this.driver = driver;
	}
}
